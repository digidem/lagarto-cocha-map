{
  "name": "lagarto-cocha-map",
  "version": "1.0.0",
  "description": "Interactive map of Siekopai ancestral territory in Lagarto Cocha",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "budo": "^9.4.7",
    "d3-request": "^1.0.5",
    "insert-css": "^2.0.0",
    "mapbox-gl": "^0.36.0",
    "sheetify": "^6.0.1",
    "tachyons": "^4.6.2",
    "yo-yo": "^1.4.0"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "budo": "^9.4.7",
    "copyfiles": "^1.2.0",
    "gh-pages": "^0.12.0",
    "mkdirp": "^0.5.1",
    "uglifyify": "^3.0.4",
    "varify": "^0.2.0",
    "yo-yoify": "^3.7.0"
  },
  "scripts": {
    "start": "budo src/index.js:bundle.js --live -d static -- -t sheetify/transform",
